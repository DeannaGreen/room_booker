<p id="notice"><%= notice %></p>

<h1>Roombookings</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Roomid</th>
      <th>Starttime</th>
      <th>Endtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @roombookings.each do |roombooking| %>
      <tr>
        <td><%= roombooking.id %></td>
        <td><%= roombooking.roomid %></td>
        <td><%= roombooking.starttime %></td>
        <td><%= roombooking.endtime %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Roombooking', new_roombooking_path %>
