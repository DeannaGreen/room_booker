{
	<% @bookings.each do |roomid , roombookings| %>
	<% i += 1 %>
	"<%= i %>": {
		"title": "Room_<%= roomid %>" ,
		"schedule": [
			 <% roombookings.each do | book| %>	
			{
			"start": "<%= book.starttime.strftime("%H:%M") %>",
			"end": "<%= book.endtime.strftime("%H:%M") %>",
			"text": "Text Area",
			"data": {}
		} <% if(book != roombookings.last) %> , <% end %>	
		<% end %>
		]
          
  	
	}
	<% end %>
}
